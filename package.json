{
  "name": "storefront-ui",
  "description": "React PWA (single page application) e-commerce UI framework",
  "scripts": {
    "publish-docs": "now --target production",
    "build": "rm -Rf dist && npx babel src/packages --out-dir dist && rm -Rf dist/*/examples && cp package.json dist && mv dist/config/storefront-ui.config__build.js dist/config/storefront-ui.config.js",
    "publish": "npm run build && npm publish dist/",
    "dev": "next --port 3333"
  },
  "dependencies": {
    "react-modal": "^3.8.1",
    "react-responsive": "^6.1.1",
    "react-visibility-sensor": "^5.0.2",
    "responsive-helpers": "^0.1.12",
    "simpleswiper": "^0.10.5"
  },
  "peerDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "@emotion/core": "^10.0.7",
    "@emotion/styled": "^10.0.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.1.6",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "@emotion/babel-preset-css-prop": "^10.0.9",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-emotion": "^10.0.7",
    "eslint": "^5.9.0",
    "eslint-plugin-react": "^7.12.4",
    "extract-react-types-loader": "^0.3.1",
    "husky": "^1.2.0",
    "markdown-to-jsx": "^6.9.3",
    "next": "^8.0.3",
    "prettier": "1.15.2",
    "pretty-quick": "^1.8.0",
    "prismjs": "^1.16.0",
    "raw-loader": "^2.0.0",
    "react-markdown": "^4.0.6",
    "webpack": "^4.26.1",
    "webpack-cli": "^3.2.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "version": "0.3.0-alpha.4"
}
